<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/view"
    android:background="@color/background">

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="30dp"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="60dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/valid2"
        android:id="@+id/view2">
    </View>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:fontFamily="sans-serif"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:layout_marginTop="50dp"
        android:text="@string/contactinformation" />

    <View
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="20dp"
        android:background="@color/line_color" />
    <View
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignStart="@+id/editText"
        android:layout_alignEnd="@+id/editText"
        android:layout_below="@+id/editText"
        android:layout_marginTop="5dp"
        android:background="@color/line_color" />
    <View
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignStart="@+id/editText5"
        android:layout_alignEnd="@+id/editText5"
        android:layout_below="@+id/editText5"
        android:layout_marginTop="5dp"
        android:background="@color/line_color" />
    <View
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_below="@+id/textView555"
        android:layout_marginTop="5dp"
        android:layout_alignStart="@+id/textView555"
        android:layout_alignEnd="@+id/textView555"
        android:background="@color/line_color" />

    <View
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_above="@+id/button2"
        android:layout_alignStart="@+id/view2"
        android:layout_alignEnd="@+id/view2"
        android:background="@color/black" />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginEnd="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="60dp"
        android:layout_alignParentBottom="true"
        android:background="@color/tw__transparent"
        android:text="Ok"
        android:textSize="18sp"
        android:textColor="@color/black"/>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Callus"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="75dp"
        android:layout_marginStart="45dp"
        />

    <TextView
        android:id="@+id/editText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        android:text="934 51 944"
        android:textColor="@color/line_color"
        android:layout_below="@+id/textView14"
        android:layout_toEndOf="@+id/textView5"
        android:layout_marginStart="15dp"
        android:layout_marginTop="75dp"
        />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Sendanemail"
        android:textSize="18sp"
        android:textColor="@color/black"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="75dp"
        android:layout_marginStart="45dp"
        />

    <TextView
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="post@kortfri.no"
        android:textSize="18sp"
        android:textColor="@color/line_color"
        android:layout_below="@+id/textView5"
        android:layout_toEndOf="@+id/textView55"
        android:layout_marginStart="15dp"
        android:layout_marginTop="75dp"
        />

    <TextView
        android:id="@+id/textView555"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/kortfriNorge"
        android:textSize="18sp"
        android:textColor="@color/line_color"
        android:layout_below="@+id/editText5"
        android:layout_marginTop="75dp"
        android:layout_centerHorizontal="true"
        />

</RelativeLayout>